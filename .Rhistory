library(C3)
library(shiny)
library(lubridate)
# helper function to create fake data: random walk with drift
# see http://stackoverflow.com/questions/24272862/generate-random-walk-with-drift-and-or-trend-in-r
getSeries <- function( n = 100, drift = 0.1, walk = 4, scale = 100){
y <- scale * cumsum(rnorm(n= n, mean = drift, sd=sqrt(walk)))
y <- round(y)
return(y + abs(min(y)))
}
server <- function(input, output,session) {
# make sure we get the same data each time
set.seed(1000)
# create data for 30 days
n <- 30
# start date
start_date = ymd("2016-10-01")
output$chart <- renderC3StackedAreaChart({
dataset <- data.frame(
Time  = start_date + 1:n ,
data1 = getSeries(n = n, drift = 0.3, walk = 3, scale = 30),
data2 = getSeries(n = n, drift = 0.1, walk = 4, scale = 20)
)
C3StackedAreaChart(dataset  = dataset,
types    = list(data1 = "bar", data2 = "area-spline"),
colors   = list(data1 = "green", data2 = "red"),
groups   = c("data1","data2"),
subchart = FALSE)
})
# update 1
observeEvent(input$update1, {
c3ChartProxy("chart") %>% zoomChart("2016-10-01", "2016-10-15")
})
# update 2
observeEvent(input$update2, {
c3ChartProxy("chart") %>% zoomChart("2016-10-16", "2016-10-30")
})
# update 3
observeEvent(input$update3, {
c3ChartProxy("chart") %>% zoomChart("2016-10-01", "2016-10-15") %>% setColors(list(data2 = "purple", data2 = "gray"))
})
# update 4
observeEvent(input$update4, {
c3ChartProxy("chart") %>% setColors(list(data1 = "purple", data2 = "gray"))
})
# update 5
observeEvent(input$update5, {
c3ChartProxy("chart") %>%  setColors(list(data1 = "green", data2 = "red"))
})
# update 6
observeEvent(input$update6, {
c3ChartProxy("chart") %>% zoomChart("2016-10-01", "2016-10-30")
})
}
ui <- fluidPage(
C3StackedAreaChartOutput("chart"),
actionButton("update1","go to first half of the month"),
actionButton("update2","go to second half of the month"),
actionButton("update3","go to first half of the month and update colors"),
actionButton("update4","update colors"),
actionButton("update5","reset chart colors"),
actionButton("update6","reset zoom domain")
)
shinyApp(ui = ui, server = server)
